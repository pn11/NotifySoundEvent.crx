{
  "manifest_version": 2,
  "name": "Notify sound event",
  "description": "Notify the sound event in a tab to the desktop.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "notifications"
  ]
}
